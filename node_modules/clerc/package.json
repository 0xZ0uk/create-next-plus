{
  "name": "clerc",
  "version": "0.42.2",
  "author": "Ray <i@mk1.io> (https://github.com/so1ve)",
  "type": "module",
  "description": "Clerc: The full-featured cli library.",
  "keywords": [
    "args",
    "arguments",
    "argv",
    "clerc",
    "cli",
    "terminal"
  ],
  "homepage": "https://github.com/clercjs/clerc/tree/main/packages/clerc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clercjs/clerc.git",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/clercjs/clerc/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@clerc/core": "0.42.2",
    "@clerc/plugin-help": "0.42.2",
    "@clerc/plugin-friendly-error": "0.42.2",
    "@clerc/plugin-completions": "0.42.2",
    "@clerc/plugin-strict-flags": "0.42.2",
    "@clerc/plugin-version": "0.42.2",
    "@clerc/plugin-not-found": "0.42.2"
  },
  "scripts": {
    "build": "pkgroll",
    "watch": "pkgroll --watch"
  }
}